{
    "name": "spaceace/activeextlist",
    "type": "phpbb-extension",
    "description": "Shows total number of active extensions installed in the forum statistics with a link to a new page to a list of the installed extensions with their versions and descriptions.",
    "homepage": "http://www.livemembersonly.com",
    "version": "1.0.0",
    "time": "2016-21-08",
    "keywords": [
        "phpbb",
        "extension",
        "Active Extensions List"
    ],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "spaceace",
            "email": "spaceace@livemembersonly.com",
            "homepage": "http://www.livemembersonly.com",
            "role": "Extension Developer"
        },
        {
            "name": "dmzx",
            "homepage": "http://www.dmzx-web.net",
            "email": "info@dmzx-web.net",
            "role": "Extension Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3"
    },
    "extra": {
        "display-name": "Active Extensions List",
        "soft-require": {
            "phpbb/phpbb": "3.1.*"
        },
        "version-check": {
            "host": "www.livemembersonly.com",
            "directory": "/version",
            "filename": "activeextlist.json"
        }
    }
}